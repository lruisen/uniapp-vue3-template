		"@unocss/preset-legacy-compat": "0.59.4",
		"@vue/runtime-core": "^3.5.16",
		"@vue/tsconfig": "^0.1.3",
		"dotenv": "^16.5.0",
		"esno": "^4.8.0",
		"fs-extra": "^11.3.0",
		"sass": "^1.78.0",
		"sass-loader": "^16.0.5",
		"typescript": "^4.9.5",
		"unocss": "0.58.9",
